## cross-compiling

if test "${cross_compiling}" = yes; then
  { echo "$as_me:$LINENO: checking for build C compiler" >&5
echo $ECHO_N "checking for build C compiler... $ECHO_C" >&6; }
  build_cc_works=no
  echo "int main(void) { return 0; }" > conftest.c
  if test -n "${BUILD_CC}" && "${BUILD_CC}" conftest.c -o conftest && ./conftest; then
      build_cc_works=yes;
  fi
  if test "${build_cc_works}" = no; then
    for prog in gcc cc; do
      if "${prog}" conftest.c -o conftest >/dev/null 2>&1 && ./conftest; then
        BUILD_CC="${prog}"; build_cc_works=yes; break
      fi
    done
  fi
  rm -rf conftest conftest.* core
  if test "${build_cc_works}" = no; then
    { echo "$as_me:$LINENO: result: none" >&5
echo "${ECHO_T}none" >&6; }
    { { echo "$as_me:$LINENO: error: Build C compiler doesn't work. Set BUILD_CC to a compiler capable of creating a binary native to the build machine." >&5
echo "$as_me: error: Build C compiler doesn't work. Set BUILD_CC to a compiler capable of creating a binary native to the build machine." >&2;}
   { (exit 1); exit 1; }; }
  fi
  { echo "$as_me:$LINENO: result: ${BUILD_CC}" >&5
echo "${ECHO_T}${BUILD_CC}" >&6; }
  { echo "$as_me:$LINENO: checking for build R" >&5
echo $ECHO_N "checking for build R... $ECHO_C" >&6; }
  : ${BUILD_R=R}
  if echo 'cat(R.home())'|"${BUILD_R}" --vanilla --slave >/dev/null 2>&1; then
    { echo "$as_me:$LINENO: result: ${BUILD_R}" >&5
echo "${ECHO_T}${BUILD_R}" >&6; }
  else
    { echo "$as_me:$LINENO: result: none" >&5
echo "${ECHO_T}none" >&6; }
    { { echo "$as_me:$LINENO: error: Build R doesn't work. Set BUILD_R to a native build of the same R version that you want to cross-compile." >&5
echo "$as_me: error: Build R doesn't work. Set BUILD_R to a native build of the same R version that you want to cross-compile." >&2;}
   { (exit 1); exit 1; }; }
  fi
fi



