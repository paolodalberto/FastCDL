## check accuracy of log1p
{ echo "$as_me:$LINENO: checking for working log1p" >&5
echo $ECHO_N "checking for working log1p... $ECHO_C" >&6; }
if test "${r_cv_func_log1p_works+set}" = set; then
  echo $ECHO_N "(cached) $ECHO_C" >&6
else
